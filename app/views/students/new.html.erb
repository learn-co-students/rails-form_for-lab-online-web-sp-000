<h1>Student Form</h1>
<p>Find me in app/views/students/new.html.erb</p>

<%= form_tag students_path do%>
    <label>Students First Name:</label><br>
    <%= text_field_tag :'student[first_name]' %><br>

    <label>Students Last Name:</label><br>
    <%= text_field_tag :'student[last_name]' %><br>

    <%= submit_tag "Create Student" %>
<% end %>