<h1> School Class Form </h1>

<%= form_tag school_classes_path, method: "post" do %>
  <label> School Class Title </label><br>
  <%= text_field_tag :'school_class[title]' %><br>

  <label> Room Number </label><br>
  <%= text_field_tag :'school_class[room_number]' %><br>

  <input type="submit" value="Create School class">
<% end %>
