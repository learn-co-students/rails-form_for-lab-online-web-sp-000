<h1> Student Form </h1>

<%= form_tag students_path, method: "post" do %>
  <label> First Name </label><br>
  <%= text_field_tag :'student[first_name]' %><br>

  <label> Last Name </label><br>
  <%= text_field_tag :'student[last_name]' %><br>

  <input type="submit" value="Create Student">
<% end %>
